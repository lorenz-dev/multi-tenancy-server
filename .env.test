# Test Environment Configuration

# Database (test database on port 5433)
DATABASE_URL=postgresql://postgres:postgres@localhost:5433/claims_test_db
DB_POOL_MIN=2
DB_POOL_MAX=10

# Redis (test Redis on port 6380)
REDIS_URL=redis://localhost:6380
REDIS_CACHE_TTL_CLAIM=300
REDIS_CACHE_TTL_LIST=60

# JWT
JWT_SECRET=test-secret-key
JWT_EXPIRES_IN=7d

# App
PORT=3100
WORKER_PORT=3102
NODE_ENV=test

# Logging
LOG_LEVEL=error

# Metrics
ENABLE_METRICS=false

# Cache
ENABLE_CACHE=true

# BullMQ
BULLMQ_CONCURRENCY=1
BULLMQ_MAX_RETRIES=3
BULLMQ_BACKOFF_DELAY_MS=100
