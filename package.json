{
  "name": "multi-tenant-server",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "worker": "tsx watch src/worker.ts",
    "db:generate": "drizzle-kit generate",
    "db:pull": "drizzle-kit pull",
    "start": "concurrently 'yarn dev' 'yarn worker'",
    "setup": "yarn dc:down && yarn dc:up && sleep 5 && yarn db:migrate",
    "dc:up": "docker-compose -f docker-compose.local.yml up -d",
    "dc:down": "docker-compose -f docker-compose.local.yml down",
    "db:migrate": "drizzle-kit migrate --config=drizzle.config.ts",
    "test": "yarn test:unit && yarn test:integration",
    "test:unit": "vitest --config=vitest.unit.config.ts --run",
    "test:integration": "vitest --config=vitest.integration.config.ts --run",
    "test:setup": "yarn test:dc:down && yarn test:dc:up && sleep 5 && yarn test:db:migrate",
    "test:dc:up": "docker-compose -f docker-compose.test.yml up -d",
    "test:dc:down": "docker-compose -f docker-compose.test.yml down",
    "test:db:migrate": "drizzle-kit migrate --config=drizzle.config.test.ts"
  },
  "dependencies": {
    "bullmq": "^5.1.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.45.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "postgres": "^3.4.3",
    "prom-client": "^15.1.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.6",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitest/coverage-v8": "^1.1.0",
    "concurrently": "^9.2.1",
    "drizzle-kit": "^0.31.8",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
